{
	"moduleFileExtensions": ["js", "json", "ts"],
	"rootDir": ".",
	"testEnvironment": "node",
	"testRegex": ".e2e-spec.ts$",
	"transform": {
		"^.+\\.(t|j)s$": "ts-jest"
	},
	"moduleNameMapper": {
		"^@db/(.*)$": "<rootDir>/../src/db/$1",
		"^@movie/(.*)$": "<rootDir>/../src/movie/$1"
	},
	"moduleDirectories": ["node_modules", "src"]
}
